<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <RootNamespace>SAI_Launcher</RootNamespace>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>media\appicon.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="media\appicon.ico" />
    <None Remove="media\fondo.png" />
    <None Remove="res\release.zip" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="media\appicon.ico" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="media\appicon.ico" />
    <EmbeddedResource Include="media\fondo.png" />
    <EmbeddedResource Include="res\release.zip" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="fondo.png" />
  </ItemGroup>

</Project>
